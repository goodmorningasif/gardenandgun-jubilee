<div class="copy-to-img-layout wow fadeIn"
  data-wow-duration="1s">
  <div class="copy">
    <div class="sectional">
    {{# copy_icon}} 
      <div class="icon wow swing"
        data-wow-duration="2s" data-wow-delay="3s"
        data-wow-offset="30">
      	{{{copy_icon}}}
      </div>
    {{/ copy_icon}} 
      <h3>
      	{{copy_sect}}
      </h3>
    </div>
    <div class="headline">
    	{{{copy_head}}}
    </div>
    <div class="para">
    	{{{copy_para}}}
    </div>
  </div>
  {{> images}}
</div>